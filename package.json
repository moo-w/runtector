{
  "name": "runtector",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6",
  "description": "JavaScript runtime detector",
  "author": "Moozon <muzhenweizh@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/moo-w/runtector#readme",
  "repository": {
    "type": "git",
    "url": "git+github.com/moo-w/runtector.git"
  },
  "keywords": [
    "runtimes",
    "detector",
    "js",
    "javascript",
    "node",
    "deno",
    "bun"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "nr test:all",
    "test:all": "nr test:bun && nr test:deno && nr test:vitest",
    "test:vitest": "vitest --run",
    "test:bun": "bun ./test/bun.ts",
    "test:deno": "deno ./test/deno.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@types/node": "^22.13.14",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "happy-dom": "^17.4.4",
    "jsdom": "^26.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
